		"redux": "^4.0.5",
		"redux-logger": "^3.0.6",
		"redux-persist": "^6.0.0",
		"redux-thunk": "^2.3.0",
		"styled-components": "^5.2.1",
		"styled-normalize": "^8.0.7",
		"styled-reset": "^4.3.4",
		"uuid": "^8.3.2"
	},
