{
	"name": "minor-story-creation",
	"version": "1.0.0",
	"private": true,
	"description": "The story creation project from Max Altena for the Minor Digital Experience Design",
	"keywords": [
		"maxaltena",
		"story-creation",
		"minor-dxd"
	],
	"homepage": "/",
	"bugs": {
		"url": "https://github.com/maxaltena/minor-story-creation/issues"
	},
	"repository": {
